setcpm(140/4)

const tr= chooseCycles("<0 1 0 1>/2")

$: note("[eb1,eb2]!16 [g2, d1]!16 [bb2, bb1]!8 [f2, f1]!8")
  .s("supersaw")
  .lpf(slider(1695,0,3000))
  .lpq(2)
  .detune(0.05)
  .slow(8)
  .postgain(slider(0.84,0,2))
._punchcard()


$:  note("{d2 bb2 c3 bb3 d3 eb3}!8| {c3 f3 bb3 eb2 c2 f2}!8 | {d4 eb4 bb4 bb3 eb3 d2}!8")
  .slow(4)
  .s("supersaw")
  .lpf(slider(1286.1,100,3000))
  .hpf(600)
  .struct("1<3 2 5>,6")
  .gain("1 0.7 0.9 0.6")
  .sometimes(loopAt(1.5))
  .postgain(slider(0.736,0,2))
._punchcard()

$:  note("{d2 bb2 c3 bb3 d3 eb3}!8| {c3 f3 bb3 eb2 c2 f2}!8 | {d4 eb4 bb4 bb3 eb3 d2}!8")
  .slow(4)
  .s("supersaw")
  .lpf(slider(1210.7,100,3000))
  .hpf(1000)
  .jux(rev)
  .delay(0.2)
  .delayfb(0.3)
  .pan(sine.range(0.2, 0.8).slow(3))
  .postgain(slider(1.494,0,3))
._punchcard()

